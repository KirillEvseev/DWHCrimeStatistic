  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT [Region]
	,[SubRegion]
	,[CalendarYear]
		,SUM([CrimeCount]) as CrimeCount
	FROM [dwh].[FactCrime] AS FC
		JOIN [dwh].[DimGeography] AS DG
			ON FC.[DimGeographyKey] = DG.[DimGeographyKey]
		JOIN [dwh].[DimDate] AS DD
			ON DD.[DateSK] = FC.DimDateKey
	GROUP BY [Region],[SubRegion],[CalendarYear]</CommandText>
      </Query>
      <Fields>
        <Field Name="Region">
          <DataField>Region</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SubRegion">
          <DataField>SubRegion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CalendarYear">
          <DataField>CalendarYear</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CrimeCount">
          <DataField>CrimeCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
